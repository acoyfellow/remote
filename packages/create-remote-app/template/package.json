{
  "name": "test-local-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "alchemy dev",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:better-auth": "bunx @better-auth/cli generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:local": "wrangler d1 migrations apply test-local-template-db --local",
    "db:remote": "wrangler d1 migrations apply test-local-template-db --remote"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.1.3",
    "@sveltejs/kit": "^2.28.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.2",
    "@tailwindcss/vite": "^4.1.11",
    "drizzle-kit": "^0.31.4",
    "svelte": "^5.38.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "alchemy": "^0.60.2",
    "better-auth": "1.3.6",
    "drizzle-orm": "^0.44.4"
  },
  "type": "module"
}
